<?xml version="1.0" encoding="utf-8"?>
<OfficeApp
    xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
    xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides"
    xsi:type="TaskPaneApp"
>
    <Id>8aba1898-5d93-4701-b178-0205c74e936a</Id>
    <Version>1.0.0.0</Version>
    <ProviderName>Giovanni Luca Lo Magno</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>
    <DisplayName DefaultValue="SWordy" />
    <Description DefaultValue="Connect Word to Stata" />
    <SupportUrl DefaultValue="https://sourceforge.net/projects/swordy/" />
    <IconUrl DefaultValue="https://swordy.sourceforge.io/img/swordy_icon_32x32.png" />
    <Hosts>
        <Host Name="Document"/>
    </Hosts>
    <DefaultSettings>
        <SourceLocation DefaultValue="https://swordy.sourceforge.io/home.html" />
    </DefaultSettings>
    <Permissions>ReadWriteDocument</Permissions>
    <VersionOverrides
        xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides"
        xsi:type="VersionOverridesV1_0"
    >   
        <Hosts>
            <Host xsi:type="Document">
                <DesktopFormFactor>
                    <GetStarted>
                        <Title resid="swordy.getStarted.title"/>						
                        <Description resid="swordy.getStarted.description"/>
                        <LearnMoreUrl resid="swordy.getStarted.learnMoreUrl"/>
                    </GetStarted>
                    <!-- The function file is required by the manifest schema, but it is not used by SWordy -->               
                    <FunctionFile resid="swordy.functionFile" />
                    <ExtensionPoint xsi:type="PrimaryCommandSurface">
                        <CustomTab id="swordy.tab">
                            <Group id="swordy.insertGroup">
                                <Label resid="swordy.insertGroup.label" />
                                <Icon>
                                    <bt:Image size="16" resid="swordy.testButton.icon" />
                                    <bt:Image size="32" resid="swordy.testButton.icon" />
                                    <bt:Image size="80" resid="swordy.testButton.icon" />
                                </Icon>                
                                
                                <Control xsi:type="Button" id="swordy.insertScalarBtn">
                                    <Label resid="swordy.insertScalarBtn.label" />
                                    <Supertip>
                                        <Title resid="swordy.temp.supertip.title" />
                                        <Description resid="swordy.temp.supertip.des" />
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="swordy.scalar16.icon" />
                                        <bt:Image size="32" resid="swordy.scalar32.icon" />
                                        <bt:Image size="64" resid="swordy.scalar64.icon" />
                                        <bt:Image size="80" resid="swordy.scalar80.icon" />
                                    </Icon>
                                    <Action xsi:type="ShowTaskpane">
                                        <TaskpaneId>insertScalarTP</TaskpaneId>                                     
                                        <SourceLocation resid="swordy.insertScalarTP.url" />
                                    </Action>
                                </Control>
                                
                                <Control xsi:type="Button" id="swordy.insertMatrixBtn">
                                    <Label resid="swordy.insertMatrixBtn.label" />
                                    <Supertip>
                                        <Title resid="swordy.temp.supertip.title" />
                                        <Description resid="swordy.temp.supertip.des" />
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="swordy.matrix16.icon" />
                                        <bt:Image size="32" resid="swordy.matrix32.icon" />
                                        <bt:Image size="64" resid="swordy.matrix64.icon" />
                                        <bt:Image size="80" resid="swordy.matrix80.icon" />
                                    </Icon>
                                    <Action xsi:type="ShowTaskpane">
                                        <TaskpaneId>insertMatrixTP</TaskpaneId>                                     
                                        <SourceLocation resid="swordy.insertMatrixTP.url" />
                                    </Action>
                                </Control>
                            </Group>
                            <Group id="swordy.bindingsGroup">
                                <Label resid="swordy.bindingsGroup.label" />
                                <Icon>
                                    <bt:Image size="16" resid="swordy.testButton.icon" />
                                    <bt:Image size="32" resid="swordy.testButton.icon" />
                                    <bt:Image size="80" resid="swordy.testButton.icon" />
                                </Icon>
                                
                                <Control xsi:type="Button" id="swordy.bindings">
                                    <Label resid="swordy.bindings.label" />
                                    <Supertip>
                                        <Title resid="swordy.temp.supertip.title" />
                                        <Description resid="swordy.temp.supertip.des" />
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="swordy.binding16.icon" />
                                        <bt:Image size="32" resid="swordy.binding32.icon" />
                                        <bt:Image size="64" resid="swordy.binding64.icon" />
                                        <bt:Image size="80" resid="swordy.binding80.icon" />
                                    </Icon>
                                    <Action xsi:type="ShowTaskpane">
                                        <TaskpaneId>bindings</TaskpaneId>                                     
                                        <SourceLocation resid="swordy.bindings.url" />
                                    </Action>
                                </Control>                                
                                                                
                                <Control xsi:type="Button" id="swordy.syncDocBtn">
                                    <Label resid="swordy.syncDocBtn.label" />
                                    <Supertip>
                                        <Title resid="swordy.temp.supertip.title" />
                                        <Description resid="swordy.temp.supertip.des" />
                                    </Supertip>
                                    <Icon>
                                        <bt:Image size="16" resid="swordy.syncDoc16.icon" />
                                        <bt:Image size="32" resid="swordy.syncDoc32.icon" />
                                        <bt:Image size="64" resid="swordy.syncDoc64.icon" />
                                        <bt:Image size="80" resid="swordy.syncDoc80.icon" />
                                    </Icon>
                                    <Action xsi:type="ShowTaskpane">
                                        <TaskpaneId>bindingsListTP</TaskpaneId>                                     
                                        <SourceLocation resid="swordy.syncDoc.url" />
                                    </Action>
                                </Control>                                                           
                            </Group>
                            
                            <Label resid="swordy.tab.label" />
                        </CustomTab>
                    </ExtensionPoint>
                </DesktopFormFactor>
            </Host>
        </Hosts>
        <Resources>
            <bt:Images>
                <bt:Image id="swordy.testButton.icon" DefaultValue="https://i.imgur.com/FkSShX9.png" />
                <bt:Image id="swordy.scalar16.icon" DefaultValue="https://swordy.sourceforge.io/img/scalar_icon/scalar_icon_16x16.png" />
                <bt:Image id="swordy.scalar32.icon" DefaultValue="https://swordy.sourceforge.io/img/scalar_icon/scalar_icon_32x32.png" />
                <bt:Image id="swordy.scalar64.icon" DefaultValue="https://swordy.sourceforge.io/img/scalar_icon/scalar_icon_64x64.png" />
                <bt:Image id="swordy.scalar80.icon" DefaultValue="https://swordy.sourceforge.io/img/scalar_icon/scalar_icon_80x80.png" />
                <bt:Image id="swordy.matrix16.icon" DefaultValue="https://swordy.sourceforge.io/img/matrix_icon/matrix_icon_16x16.png" />
                <bt:Image id="swordy.matrix32.icon" DefaultValue="https://swordy.sourceforge.io/img/matrix_icon/matrix_icon_32x32.png" />
                <bt:Image id="swordy.matrix64.icon" DefaultValue="https://swordy.sourceforge.io/img/matrix_icon/matrix_icon_64x64.png" />
                <bt:Image id="swordy.matrix80.icon" DefaultValue="https://swordy.sourceforge.io/img/matrix_icon/matrix_icon_80x80.png" />
                <bt:Image id="swordy.binding16.icon" DefaultValue="https://swordy.sourceforge.io/img/binding_icon/binding_icon_16x16.png" />
                <bt:Image id="swordy.binding32.icon" DefaultValue="https://swordy.sourceforge.io/img/binding_icon/binding_icon_32x32.png" />
                <bt:Image id="swordy.binding64.icon" DefaultValue="https://swordy.sourceforge.io/img/binding_icon/binding_icon_64x64.png" />
                <bt:Image id="swordy.binding80.icon" DefaultValue="https://swordy.sourceforge.io/img/binding_icon/binding_icon_80x80.png" />
                <bt:Image id="swordy.syncDoc16.icon" DefaultValue="https://swordy.sourceforge.io/img/sync_document_icon/sync_document_icon_16x16.png" />
                <bt:Image id="swordy.syncDoc32.icon" DefaultValue="https://swordy.sourceforge.io/img/sync_document_icon/sync_document_icon_32x32.png" />
                <bt:Image id="swordy.syncDoc64.icon" DefaultValue="https://swordy.sourceforge.io/img/sync_document_icon/sync_document_icon_64x64.png" />
                <bt:Image id="swordy.syncDoc80.icon" DefaultValue="https://swordy.sourceforge.io/img/sync_document_icon/sync_document_icon_80x80.png" />
            </bt:Images>
            <bt:Urls>
                <bt:Url id="swordy.functionFile" DefaultValue="https://swordy.sourceforge.io/function_file.html" />
                <bt:Url id="swordy.getStarted.learnMoreUrl" DefaultValue="https://sourceforge.net/projects/swordy/" />
                <bt:Url id="swordy.insertScalarTP.url" DefaultValue="https://swordy.sourceforge.io/insert_scalar_taskpane.html" />
                <bt:Url id="swordy.insertMatrixTP.url" DefaultValue="https://swordy.sourceforge.io/insert_matrix_taskpane.html" />                
                <bt:Url id="swordy.bindings.url" DefaultValue="https://swordy.sourceforge.io/bindings_taskpane.html" />
                <bt:Url id="swordy.syncDoc.url" DefaultValue="https://swordy.sourceforge.io/sync_document_taskpane.html" />
            </bt:Urls>
            <bt:ShortStrings>
                <bt:String id="swordy.getStarted.title" DefaultValue="SWordy add-in was succesfully loaded" />
                <bt:String id="swordy.tab.label" DefaultValue="SWordy" />
                <bt:String id="swordy.insertGroup.label" DefaultValue="Insert Stata data" />
                <bt:String id="swordy.bindingsGroup.label" DefaultValue="Bindings" />                
                <bt:String id="swordy.insertScalarBtn.label" DefaultValue="Insert scalar" />
                <bt:String id="swordy.insertMatrixBtn.label" DefaultValue="Insert matrix" />
                <bt:String id="swordy.bindings.label" DefaultValue="Bindings" />
                <bt:String id="swordy.syncDocBtn.label" DefaultValue="Sync document" />
                <bt:String id="swordy.temp.supertip.title" DefaultValue="Temp supertip title" />
            </bt:ShortStrings>
            <bt:LongStrings>                
                <bt:String id="swordy.getStarted.description" DefaultValue="Get going by opening SWordy tab on the Ribbon" />  
                <bt:String id="swordy.temp.supertip.des" DefaultValue="Temp supertip description" />
            </bt:LongStrings>
        </Resources>
    </VersionOverrides>
</OfficeApp>
